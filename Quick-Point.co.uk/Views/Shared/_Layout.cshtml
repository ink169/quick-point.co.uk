<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quick-Point.co.uk</title>
	
	<link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
	
	<!-- Google Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <!-- ======= Mobile Menu ======= -->
    <div class="site-mobile-menu site-navbar-target">
        <div class="site-mobile-menu-header">
            <div class="site-mobile-menu-close mt-3">
                <span class="icofont-close js-menu-toggle"></span>
            </div>
        </div>
        <div class="site-mobile-menu-body"></div>
    </div>

    <div id="sticky-wrapper" class="sticky-wrapper" style="height: 85px;">
        <!-- ======= Header ======= -->
        <header class="site-navbar js-sticky-header site-navbar-target" role="banner">

            <div class="container">
                <div class="row align-items-center">

                    @*<div class="col-6 col-lg-2">
                                <h1 class="mb-0 site-logo"><a href="/Home/Index" class="mb-0">Home</a></h1>
                        </div>*@


                    <div class="col-12 col-md-12 d-none d-lg-block">
                        <nav class="site-navigation position-relative text-right" role="navigation">
                            @*<nav class="site-navigation position-relative" role="navigation">*@
                            <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">
                                @*<li><a href="/Home/Index"><img src="~/Content/images/SL.jpg" alt="" class="hero-section-img" height="53" width="212" /></a></li>*@
                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                <li>@Html.ActionLink("Resources", "Resources", "Home")</li>
                                <li>@Html.ActionLink("Q&A", "FAQ", "Home")</li>
                                <li>@Html.ActionLink("About", "About", "Home")</li>
                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                                <li><a href="/Home/FF"><img src="~/assets/images/Nav-Scrolled.png" alt="" class="hero-section-img" /></a> </li>
                                @*<li>@Html.ActionLink("Free Financial Analysis", "FFA", "Home")</li>*@
                            </ul>
                        </nav>
                    </div>

                    <div class="col-12 d-inline-block d-lg-none ml-md-0 py-3" style="position: relative; top: 3px;">
                        <a href="#" class="burger site-menu-toggle js-menu-toggle" data-toggle="collapse" data-target="#main-navbar">
                            <span></span>
                        </a>
                    </div>

                    @*<div style="margin-top: -13px;">
                            <a href="/Home/FFA">
                                <img src="~/Content/images/FA_sml.png" alt="" class="hero-section-img" />
                            </a>
                        </div>*@

                </div>
            </div>
        </header>
    </div>
    <div>
        @RenderBody()

        <!-- ======= Footer ======= -->
        <footer class="footer" role="contentinfo">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 mb-12 mb-md-0">
                        <h3>About Quick-Point.co.uk</h3>
                        <p>
                            This chat bot is designed to give free advice regarding business support during the Corona Virus crisis. Answers have been provided and tested by experienced, qualified and fully regulated law and accounting specialists supporting this project, reducing the need for face to face meetings. However, this chatbot is not a replacement for these services provided and should further information be required, please contact us via the contact page or via the ‘Help’ function. Quickpoint is not a substitute for a lawyer or an accountant and aims only to provide relevant information issued by the UK Government during this Corona Virus crisis.
                        </p>
                        @*<p class="social">
                                <span><a href="#"><img src="~/assets/images/SBLogo.png" /></a></span>
                                <span><a href="#"><img src="~/assets/images/sterlingLawLogo.jpg" /></a></span>
                                <span><a href="#"><img src="~/assets/images/InnovateUKLogo.png" /></a></span>
                            </p>*@

                        @*<p class="social">
                                <a href="#"><span class="icofont-twitter"></span></a>
                                <a href="#"><span class="icofont-facebook"></span></a>
                                <a href="#"><span class="icofont-dribbble"></span></a>
                                <a href="#"><span class="icofont-behance"></span></a>
                            </p>*@
                    </div>
                    @*<div class="col-md-6 ml-auto">
                            <div class="row site-section pt-0">
                                <div class="col-md-4 mb-4 mb-md-0">
                                    <h3>Navigation</h3>
                                    <ul class="list-unstyled">
                                        <li>@Html.ActionLink("Resources", "Resources", "Home")</li>
                                        <li>@Html.ActionLink("Q&A", "FAQ", "Home")</li>
                                        <li>@Html.ActionLink("Meet The Team", "About", "Home")</li>
                                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                                    </ul>
                                </div>
                            </div>
                        </div>*@
                </div>
                <div class="row justify-content-center text-center">
                    <div class="col-xs-12 col-sm-4 col-no-padding">
                        <img class="thumbnail img-responsive" src="~/assets/images/SBLogo.png">
                    </div>
                    <div class="col-xs-12 col-sm-4 col-no-padding">
                        <img class="thumbnail img-responsive" src="~/assets/images/sterlingLawLogo.jpg">
                    </div>
                    <div class="col-xs-12 col-sm-4 col-no-padding">
                        <img class="thumbnail img-responsive" src="~/assets/images/InnovateUKLogo.png">
                    </div>
                </div>

                <div class="row justify-content-center text-center" style="padding-top:20px;">
                    <div class="col-md-4">
                        <h3 class="copyright">&copy; Sterling &amp; Beanland @DateTime.Now.Year. All Rights Reserved</h3>
                    </div>
                    <div class="col-md-4">
                        <h3 class="copyright">@Html.ActionLink("Subscribe To Newsletter", "Subscribe", "Home")</h3>
                    </div>
                    <div class="col-md-4">
                        <h3 class="copyright">@Html.ActionLink("Terms of Use", "Terms", "Home")/@Html.ActionLink("Privacy Policy", "Privacy", "Home")</h3>
                    </div>

                </div>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-171669009-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-171669009-1');
    </script>

</body>
</html>
